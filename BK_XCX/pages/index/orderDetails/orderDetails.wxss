.orderStatus-box {
  width: 576rpx;
  height: 90rpx;
  margin: 32rpx 140rpx 1rpx 172rpx;
}

.orderStatus {
  background: rgb(253, 245, 232);
  overflow: hidden;
}

.orderStatus-box-photo, .orderStatus-box-photo image {
  width: 56rpx;
  height: 56rpx;
  float: left;
  /* background: red; */
}

.orderStatus-box-text {
  float: left;
  margin-left: 20rpx;
}

.orderStatus-box-text view.one {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #ffbb3f;
  letter-spacing: 0;
  line-height: 30rpx;
  margin-bottom: 10rpx;
}

.recommend_scroll_x_box {
  height: 210rpx;
  white-space: nowrap;
  display: flex;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.recommend_hot_box {
  width: 183rpx;
  height: 200rpx;
  display: inline-block;
  position: relative;
}

.recommend_hot_image {
  width: 230rpx;
  height: 143rpx;
}

.orderStatus-box-text view.two {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #ffbb3f;
  letter-spacing: 0;
  line-height: 11px;
}

.content {
  width: 100%;
  height: 200rpx;
  display: flex;
  background: #fafafd;
}

.content .content_left {
  flex: 0.2;
  background: #d8d8d8;
  border: 1px solid #ebebeb;
  border-radius: 12rpx;
  width: 138rpx;
  height: 138rpx;
  margin: 30rpx 22rpx;
}

.content .content_left image {
  width: 138rpx;
  height: 138rpx;
}

.content .content_right {
  flex: 1;
}

.content .content_right .subject {
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #262626;
  letter-spacing: 0;
  line-height: 32rpx;
  text-align: left;
  margin: 30rpx 0 20rpx 0;
  overflow: hidden;
}

.content .content_right .payMent {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #b8b8b8;
  letter-spacing: 0;
  line-height: 24rpx;
  text-align: left;
}

.content .content_right .userPlay {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #666;
  letter-spacing: 0;
  line-height: 24rpx;
  text-align: right;
  margin: 40rpx 40rpx 30rpx 0;
}

.tirm {
  background: #f9f9f9;
  box-shadow: 0 -1px 0 0 rgba(222, 222, 222, 0.50);
  width: 750rpx;
  height: 10px;
}

.batch_progress_header {
  width: 100%;
  height: 90rpx;
  background-color: #fff;
}

.details {
  height: 130rpx;
}

.batch_progress_header .batch_progress_header_left {
  float: left;
  width: 545rpx;
  height: 32rpx;
  margin: 30rpx;
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #262626;
  letter-spacing: 0;
  line-height: 16px;
  text-align: left;
}

#address {
  font-family: PingFangSC-Regular;
  font-size: 13px;
  color: #666;
  letter-spacing: 0;
  line-height: 30rpx;
  text-align: left;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 34rpx;
  width: 590rpx;
  overflow: hidden;
}

.batch_progress_header_left .pd {
  border-radius: 49px;
  width: 5px;
  height: 16px;
  background: #ffbb3f;
  display: block;
  float: left;
  margin-right: 18rpx;
}

.batch_progress_header_right {
  float: right;
  margin: 30rpx;
  width: 24rpx;
  height: 24rpx;
  /* background-color: black; */
}

.batch_progress_header_right image {
  width: 24rpx;
  height: 24rpx;
  display: block;
}

.solid {
  background: #f5f5f8;
  width: 750rpx;
  height: 1px;
}

.payment_cycle {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #ffbb3f;
  letter-spacing: 0;
  line-height: 12px;
  text-align: center;
}

.clear_float {
  clear: both;
}

.floor_one {
  height: 28rpx;
  width: 690rpx;
  margin: 40rpx 40rpx 40rpx 30rpx;
}

.floor_one_left {
  float: left;
}

.floor_one_right {
  float: right;
}

.floor_one_right, .floor_one_left {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #b8b8b8;
  letter-spacing: 0;
  line-height: 14px;
  text-align: left;
}

.footer {
  background: #f9f9f9;
  box-shadow: 0 -1px 0 0 rgba(222, 222, 222, 0.50);
  width: 750rpx;
  height: 224rpx;
  overflow: hidden;
}

.footer .notes {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #b8b8b8;
  letter-spacing: 0;
  text-align: left;
  margin: 40rpx 480rpx 40rpx 30rpx;
}

.footer image {
  width: 34rpx;
  height: 32rpx;
}

.button {
  width: 660rpx;
  height: 64rpx;
  margin: 30rpx 40rpx 40rpx 46rpx;
}

.button .button_left {
  float: left;
  border: 1px solid #ffbb3f;
  border-radius: 4px;
  width: 148px;
  height: 30px;
  text-align: center;
}

.button .button_right {
  float: right;
  border: 1px solid #ffbb3f;
  border-radius: 4px;
  width: 148px;
  height: 30px;
  text-align: center;
}

.button_right text, .button_left text {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #ffbc3f;
  letter-spacing: 0;
  line-height: 14px;
  text-align: left;
}

.footer_floor_last {
  background: #fff;
  box-shadow: 0 -1px 0 0 #f0f0f0;
  width: 750rpx;
  height: 49px;
}

.footer_floor_last .left {
  float: left;
  margin: 40rpx 0 36rpx 120rpx;
  font-family: PingFangSC-Regular;
  font-size: 15px;
  color: #666;
  letter-spacing: 0;
  line-height: 11px;
  text-align: center;
}

.footer_floor_last .right {
  float: right;
  background-image: linear-gradient(-90deg, #ffd842 0%, #ffbb3f 100%);
  width: 188px;
  height: 49px;
}

.footer_floor_last .right .one {
  font-family: PingFangSC-Regular;
  font-size: 15px;
  color: #fff;
  letter-spacing: 0;
  line-height: 15px;
  text-align: center;
  margin: 20rpx 0 4rpx 0;
}

.footer_floor_last .right .two {
  font-family: PingFangSC-Regular;
  font-size: 11px;
  color: #fff;
  letter-spacing: 0;
  line-height: 11px;
  text-align: center;
}

.box {
  position: relative;
}

#reminder_box {
  position: absolute;
  width: 100%;
  height: 2270rpx;
  background: rgba(0, 0, 0, .5);
  top: 0;
}

.reminder {
  width: 560rpx;
  height: 490rpx;
  overflow: hidden;
  opacity: 1;
  position: fixed;
  background: #fff;
  top: 440rpx;
  left: 90rpx;
  border-radius: 10rpx;
  z-index: 99;
}

.reminder_header {
  text-align: center;
  font-family: PingFangSC-Regular;
  width: 160rpx;
  height: 48rpx;
  margin: 34rpx 212rpx;
  font-size: 34rpx;
}

.reminder_content {
  width: 480rpx;
  height: 280rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  margin: 0 40rpx;
}
.cancel_order_box {
  height: 412rpx;
}
.cancel_order_s {
  height: 200rpx;
}
.reminder_footer {
  width: 100%;
  height: 87rpx;
  text-align: center;
  line-height: 87rpx;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: orange;
  border-top: 1px solid #ccc;
}

.order_num {
  width: 50rpx;
  height: 50rpx;
  border-radius: 100%;
  margin: 20rpx auto;
  line-height: 50rpx;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #fff;
  letter-spacing: 0;
  text-align: center;
}

.judging_lines {
  width: 136rpx;
  height: 2px;
  display: block;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.judging_lines_box {
  position: absolute;
  top: 45rpx;
  left: 117rpx;
}
.determine_payment ,
 .cancellation_payment{
  text-align: center;
  width: 276rpx;
  height: 90rpx;
  display: block;
}
.cancellation_payment
{
  float: right;
  border-left: 1px solid #ccc;
}
.determine_payment {
  float: left;
}
.close_float {
  clear: both;
}
.yellow {
  background: #ffbb3f;
}

.gray {
  background: #b8b8b8;
}

.red {
  background: red;
}

.order_num_date_status {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #666;
  letter-spacing: 0;
  line-height: 14px;
  text-align: center;
  margin: 24rpx;
}

.order_num_date_child {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #b8b8b8;
  letter-spacing: 0;
  line-height: 10px;
  text-align: center;
}

#close_order {
  width: 120rpx;
  height: 45rpx;
  float: right;
  font-size: 24rpx;
  color: black;
  text-align: center;
  line-height: 45rpx;
  border: 1px solid #ccc;
  margin: 20rpx 20rpx 0 0;
  font-family: PingFangSC-Regular;
}
